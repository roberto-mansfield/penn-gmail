<table class="check-user">
    <tr>
        <th>PennID:</th>
        <td>{{ userinfo.getPennId }}</td>
    </tr>
    <tr>
        <th>Pennkey:</th>
        <td>{{ userinfo.getPennkey }}</td>
    </tr>
    <tr>
        <th>Name:</th>
        <td>{{ userinfo.getFirstName }} {{ userinfo.getLastName }}</td>
    </tr>
    <tr>
        <th>Forwarding:</th>
        <td>{{ forwards|join('<br/>') }}</td>
    </tr>    
    <tr>
        <th>Account Check:</th>
        <td>
            {% for key,value in chkInfo.getData %}
                {{ key }} = {{ value }}<br/>
            {% endfor %}
        </td>
    </tr>    
</table>

